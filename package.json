{
  "repository": {
    "type": "git",
    "url": "https://github.com/lavenderx/electron-qq.git"
  },
  "keywords": [
    "electron",
    "qq",
    "vue",
    "webpack"
  ],
  "author": "Zongzhi Bai <dolphineor@gmail.com> (https://github.com/lavenderx)",
  "license": "MIT",
  "homepage": "https://github.com/lavenderx/electron-qq",
  "scripts": {
    "postinstall": "cd app && npm install",
    "start": "node build/dev-runner.js",
    "dev:server": "node build/dev-server.js",
    "dev:client": "cross-env HOT=1 webpack --hide-modules --config build/webpack.dev-background.conf.js && cross-env HOT=1 electron",
    "build": "rimraf dist && mkdirp dist && webpack --progress --hide-modules --config build/webpack.pro-build.conf.js",
    "package": "node build/package.js",
    "package:osx": "node build/package.js --platform=darwin --arch=x64",
    "package:mas": "node build/package.js --platform=mas",
    "package:win": "node build/package.js --platform=win32",
    "package:linux": "node build/package.js --platform=linux",
    "release": "npm run build && npm run package",
    "gulp": "gulp"
  },
  "devDependencies": {
    "babel-core": "^6.10.4",
    "babel-loader": "^6.2.4",
    "babel-plugin-transform-runtime": "^6.9.0",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-stage-2": "^6.11.0",
    "babel-runtime": "^6.9.2",
    "chromedriver": "^2.21.2",
    "connect-history-api-fallback": "^1.2.0",
    "copy-webpack-plugin": "^3.0.1",
    "cross-env": "1.0.8",
    "cross-spawn": "^4.0.0",
    "css-loader": "^0.23.1",
    "debug-menu": "^0.4.0",
    "devtron": "^1.2.1",
    "electron-packager": "^7.1.0",
    "electron-prebuilt": "1.2.5",
    "eslint": "^2.13.1",
    "eslint-config-airbnb-base": "^3.0.1",
    "eslint-friendly-formatter": "^2.0.5",
    "eslint-loader": "^1.3.0",
    "eslint-plugin-html": "^1.5.1",
    "eslint-plugin-import": "^1.10.0",
    "eventsource-polyfill": "^0.9.6",
    "extract-text-webpack-plugin": "^1.0.1",
    "file-loader": "^0.8.5",
    "gulp": "^3.9.1",
    "gulp-util": "^3.0.7",
    "html-webpack-plugin": "^2.21.0",
    "http-proxy-middleware": "^0.16.0",
    "i18next": "^3.3.1",
    "inject-loader": "^2.0.1",
    "isparta-loader": "^2.0.0",
