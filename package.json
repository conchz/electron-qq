    "json-loader": "^0.5.4",
    "mkdirp": "^0.5.1",
    "moment": "2.13.0",
    "ncp": "^2.0.0",
    "nightwatch": "^0.9.5",
    "node-sass": "^3.8.0",
    "pinyin": "^2.7.4",
    "phantomjs-prebuilt": "^2.1.7",
    "rimraf": "^2.5.2",
    "sass-loader": "^3.2.2",
    "selenium-server": "2.53.0",
    "url-loader": "^0.5.7",
    "vue": "^1.0.26",
    "vue-hot-reload-api": "^1.3.2",
    "vue-html-loader": "^1.2.3",
    "vue-i18n": "^4.0.1",
